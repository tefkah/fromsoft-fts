{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "X-Test-Header",
          "value": "test-value"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|wasm|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=31536000, stale-while-revalidate=86400, immutable"
        },
        {
          "key": "X-Test-Header",
          "value": "test-value-2"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
